{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "This is an Eleventy starter for creating websites",
  "main": ".eleventy.js",
  "scripts": {
    "clean": "rm -rf dist",
    "dev:parcel": "parcel watch src/assets/main.js --out-dir dist/assets",
    "dev:eleventy": "eleventy --serve",
    "build:sass": "parcel build src/assets/main.js --no-source-maps --out-dir dist/assets",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all clean --parallel dev:*",
    "build": "npm-run-all clean --parallel build:*",
    "test": "npx serve dist"
  },
  "keywords": [],
  "author": "Jake Ord",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.7.8",
    "autoprefixer": "^9.8.6",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.32.8",
    "sharp": "^0.27.2",
    "vanilla-lazyload": "^17.3.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": true
    }
  }
}
